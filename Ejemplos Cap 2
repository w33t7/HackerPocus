/*Figura 2.3: fig02_03.c
  Impresion de una linea mediante dos instrucciones printf*/
  #include <stdio.h>
  
  /*la funcion main inicia la ejecuciuon del programa*/
  int main()
  {
  printf ("Bienvenido");
  printf("a C!\n");
  
  return 0; /*indica que el programa termino con exito*/
  }/*fin de la funcion main*/
  
  +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
  /*Figura 2.4: fig02_04.c
    Impresion de multiples lineas mediante una sola instruccion printf*/
    #include <stdio.h>
    /*la funcion main inicia la ejecucion del programa*/
    int main()
    {
    printf("Bienvenido\na\nC!\n");
    
    return 0; /*Indica que el programa termino con exito*/
    
    } /*fin de la funcion main*/
    
      +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

    /*Figura 2.5: fig02_05.c
      Programa de suma*/
      #include <stdio.h>
      
      /*la funcion main inicia la ejecucion del programa */
    int main()
    {
    int entero1; /*primer numero a introducir por el ususario*/
    int entero2; /*segundo numero a introducir por el ususario*/
    int suma; /*variabel en la que se alamacenara la suma*/
    
    printf("Introduzca el primer entero\n"); /*indicador*/
    scanf("%d, &entero1");  /*lee un entero*/
    
    printf("Introduzca el segundo entero\n"); /*indicador*/
    scanf("%d", &entero2);             /*lee un entero*/
    
    suma = entero1 + entero2;  /*asigna el resultado a suma*/
    
    printf("La suma es %d\n", suma); /*imprime la suma*/
    
    retunr 0; /*indica que el programa termino con exito*/
  } /*fin de la funcion main*/
  +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
/*Figura 2.13: fig02_13 .c
  Uso de instrucciones if, operadores de relacion, y operadores de igualdad*/
  #include <stdio.h>
/*La funcion main inicia la ejecucion del programa*/  
main()
{
int num1; /*primer numero que lee el usuario*/
int num2; /*segundo numero que lee el ususario*/

  printf ("Introduzca dos enteros, y le dire\n");
  printf("las relaciones que satisfacen:");

scanf("%d%d", &num1, &num2); /*lectura de los enteros*/

if (num1 == num2) {
  printf("%d es igual que %d\n", num1, num2);
} /*fin del if*/

if(num1 != num2) {
  printf("%d no es igual que %d\n", num1, num2 );
}/*fin del if*/

if(num1 < num2){
  printf("%d es menor que %d\n", num1, num2);
} /*fin de if*/

if( num1 > num2){
  printf("%d es mayor que %d\n", num1 , num2);
} /*fin de if*/

if() 
 
}
